{
    "name":"PixelPeek",
    "version":"0.0.1",
    "manifest_version": 3,
    "description": "Eyedropper tool for viewing rgb or hex color codes of any webpage.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["<all_urls>"]
}